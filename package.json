{
  "name": "blog-next-contentful",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "NODE_ENV=production next build",
    "export": "npm run fetchContent && npm run build && next export",
    "fetchContent": "node lib/fetchContent.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.1",
    "contentful": "^7.14.5",
    "date-fns": "^2.14.0",
    "next": "11.1.0",
    "pretty-quick": "^2.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "dotenv": "^8.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
